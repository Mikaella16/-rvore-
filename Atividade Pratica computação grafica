      ],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Definição de um triângulo inicial\n",
        "triangulo = np.array([[0,0], [1,0], [1,1], [0,1], [0,0]])\n",
        "\n",
        "# Funções de transformações\n",
        "def transladar(figura, tx, ty):\n",
        "    return figura + np.array([tx, ty])\n",
        "\n",
        "def escalar(figura, sx, sy):\n",
        "    matriz = np.array([[sx, 0], [0, sy]])\n",
        "    return figura.dot(matriz)\n",
        "\n",
        "def rotacionar(figura, angulo):\n",
        "    rad = np.radians(angulo)\n",
        "    matriz = np.array([[np.cos(rad), -np.sin(rad)],\n",
        "                       [np.sin(rad),  np.cos(rad)]])\n",
        "    return figura.dot(matriz)\n",
        "\n",
        "# Criando versões transformadas\n",
        "triangulo_transladado = transladar(triangulo, 1, 4)\n",
        "triangulo_escalado = escalar(triangulo, 2,2)\n",
        "triangulo_rotacionado = rotacionar(triangulo, 30)\n",
        "\n",
        "# Plotando\n",
        "plt.figure(figsize=(8,8))\n",
        "plt.plot(triangulo[:,0], triangulo[:,1], 'b-', label=\"Original\")\n",
        "plt.plot(triangulo_transladado[:,0], triangulo_transladado[:,1], 'g-', label=\"Transladado\")\n",
        "plt.plot(triangulo_escalado[:,0], triangulo_escalado[:,1], 'r-', label=\"Escalado\")\n",
        "plt.plot(triangulo_rotacionado[:,0], triangulo_rotacionado[:,1], 'm-', label=\"Rotacionado\")\n",
        "\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.title(\"Transformações em Computação Gráfica - Triângulo\")\n",
        "plt.axis('equal')\n",
        "plt.show()\n"
      ]
    }
  ]
}
